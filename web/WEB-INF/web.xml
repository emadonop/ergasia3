<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>edit</servlet-name>
        <servlet-class>edit</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>edit</servlet-name>
        <url-pattern>/edit</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>loginflickr</servlet-name>
        <servlet-class>loginflickr</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginflickr</servlet-name>
        <url-pattern>/loginflickr</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>profile_page</servlet-name>
        <servlet-class>profile_page</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>profile_page</servlet-name>
        <url-pattern>/profile_page</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>stats</servlet-name>
        <servlet-class>Stats</servlet-class>
        <load-on-startup>3</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>stats</servlet-name>
        <url-pattern>/stats</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>project3</servlet-name>
        <servlet-class>project3</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>project3</servlet-name>
        <url-pattern>/project3</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>update</servlet-name>
        <servlet-class>update</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>update</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>upload_photos</servlet-name>
        <servlet-class>upload_photos</servlet-class>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>upload_photos</servlet-name>
        <url-pattern>/upload_photos</url-pattern>
    </servlet-mapping>
    
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>